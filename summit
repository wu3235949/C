//求出整数（+/-）的二进制数中1的个数
#include <stdio.h>
int main()
{
	int i,num,count;
	count =0;
	num=0;
	scanf("%d",&num);
	for(i=0;i<32;i++) {
		if(1==((num>>i)&1))
		  count++;
	}
	printf("%d",count);
	return 0;
}

#include <stdio.h>//只能求正数
int main()
{
	int num,count;
	count =0;
	num=0;
	scanf("%d",&num);
  while(num)
  { 
      if(num%2==1)
      count++;
      num=num/2;
  }
	printf("%d",count);
	return 0;
}
//---------------------------------------------------
struct stu                                                       //结构体struct
{              //成员变量
	char name[20];
	int age;
	char id[20]; 
} ;
#include <stdio.h>
int main()
{
    struct stu s1 = {"张三",20,"201800250"};    //结构体变量.成员名
        struct stu* ps=&s1;
    printf("s1 name=%s\n",(*ps).name);
       printf("s1 name=%s\n",ps->name);        //结构体指针->成员名
    printf("s1 name=%s\n",s1.name);
        printf("s1 age=%d\n",s1.age);
            printf("s1 id=%s\n",s1.id);
	return 0;
}
