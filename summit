//求出整数（+/-）的二进制数中1的个数
#include <stdio.h>
int main()
{
	int i,num,count;
	count =0;
	num=0;
	scanf("%d",&num);
	for(i=0;i<32;i++) {
		if(1==((num>>i)&1))
		  count++;
	}
	printf("%d",count);
	return 0;
}

#include <stdio.h>//只能求正数
int main()
{
	int num,count;
	count =0;
	num=0;
	scanf("%d",&num);
  while(num)
  { 
      if(num%2==1)
      count++;
      num=num/2;
  }
	printf("%d",count);
	return 0;
}
